    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, n, s, k, r, f" type="Integer" array="False" size=""/>
            <assign variable="i" expression="0"/>
            <assign variable="f" expression="1"/>
            <assign variable="s" expression="0"/>
            <input variable="n"/>
            <assign variable="k" expression="n"/>
            <while expression="n&gt;0">
                <assign variable="r" expression="n%10"/>
                <assign variable="f" expression="1"/>
                <assign variable="i" expression="0"/>
                <while expression="i&lt;r">
                    <assign variable="i" expression="i+1"/>
                    <assign variable="f" expression="f*i"/>
                </while>
                <assign variable="n" expression="n/10"/>
                <assign variable="s" expression="s+f"/>
                <output expression="s" newline="True"/>
            </while>
            <if expression="s==k">
                <then>
                    <output expression="&quot;strong num&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;not strong num&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
