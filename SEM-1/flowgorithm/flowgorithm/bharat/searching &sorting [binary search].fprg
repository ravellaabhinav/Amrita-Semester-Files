    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, t, l, m, h, n, k" type="Integer" array="False" size=""/>
            <output expression="&quot;no.of num&quot;" newline="True"/>
            <input variable="n"/>
            <declare name="a" type="Integer" array="True" size="n"/>
            <output expression="&quot;values&quot;" newline="True"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <input variable="a[i]"/>
            </for>
            <assign variable="l" expression="0"/>
            <assign variable="h" expression="n-1"/>
            <output expression="&quot;search &quot;" newline="True"/>
            <input variable="k"/>
            <assign variable="m" expression="(l+h)/2"/>
            <while expression="k!=a[m]">
                <assign variable="m" expression="(l+h)/2"/>
                <if expression="k&lt;a[m]">
                    <then>
                        <assign variable="h" expression="m-1"/>
                    </then>
                    <else>
                        <assign variable="l" expression="m+1"/>
                    </else>
                </if>
            </while>
            <output expression="&quot;num is &quot;&amp;k&amp;&quot; posi  &quot;&amp;m" newline="True"/>
        </body>
    </function>
