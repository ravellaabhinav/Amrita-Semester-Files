    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, n, s, j, c" type="Integer" array="False" size=""/>
            <output expression="&quot;enter no of values&quot;" newline="True"/>
            <input variable="n"/>
            <declare name="a, b" type="Integer" array="True" size="n"/>
            <declare name="p" type="Integer" array="True" size="2*n"/>
            <assign variable="s" expression="0"/>
            <output expression="&quot;values are..&quot;" newline="True"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <input variable="a[i]"/>
            </for>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <for variable="j" start="i" end="n-2" direction="inc" step="1">
                    <if expression="a[i]&lt;a[j+1]">
                        <then/>
                        <else>
                            <assign variable="c" expression="a[j+1]"/>
                            <assign variable="a[j+1]" expression="a[i]"/>
                            <assign variable="a[i]" expression="c"/>
                        </else>
                    </if>
                </for>
            </for>
            <if expression="a[0]==a[1]">
                <then/>
                <else>
                    <output expression="a[0]&amp;&quot;   1&quot;" newline="True"/>
                </else>
            </if>
            <for variable="i" start="0" end="n-2" direction="inc" step="1">
                <if expression="a[i]==a[i+1]">
                    <then>
                        <assign variable="s" expression="s+2"/>
                    </then>
                    <else>
                        <if expression="s==0">
                            <then>
                                <assign variable="s" expression="s+1"/>
                                <output expression="a[i]&amp;&quot;   &quot;&amp;s" newline="True"/>
                                <assign variable="s" expression="0"/>
                            </then>
                            <else>
                                <output expression="a[i]&amp;&quot;   &quot;&amp;s" newline="True"/>
                            </else>
                        </if>
                        <assign variable="s" expression="0"/>
                    </else>
                </if>
            </for>
            <if expression="a[n-2]==a[n-1]">
                <then>
                    <assign variable="s" expression="s+1"/>
                    <output expression="a[n-1]&amp;&quot;    &quot;&amp;s" newline="True"/>
                </then>
                <else>
                    <output expression="a[n-1]&amp;&quot;    1&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
