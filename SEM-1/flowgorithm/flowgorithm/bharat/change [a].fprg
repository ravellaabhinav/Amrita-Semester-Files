    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="t, p, i, n" type="Integer" array="False" size=""/>
            <output expression="&quot;enter no.of values using&quot;" newline="True"/>
            <input variable="n"/>
            <declare name="a" type="Integer" array="True" size="n"/>
            <output expression="&quot;enter values&quot;" newline="True"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <input variable="a[i]"/>
            </for>
            <output expression="&quot;press rotate to right:1/left:2&quot;" newline="True"/>
            <input variable="p"/>
            <if expression="p==1">
                <then>
                    <assign variable="t" expression="a[0]"/>
                    <for variable="i" start="0" end="n-2" direction="inc" step="1">
                        <assign variable="a[i]" expression="a[i+1]"/>
                    </for>
                    <assign variable="a[n-1]" expression="t"/>
                    <for variable="i" start="0" end="n-1" direction="inc" step="1">
                        <output expression="a[i]" newline="True"/>
                    </for>
                </then>
                <else>
                    <assign variable="t" expression="a[n-1]"/>
                    <for variable="i" start="n-1" end="1" direction="dec" step="1">
                        <assign variable="a[i]" expression="a[i-1]"/>
                    </for>
                    <assign variable="a[0]" expression="t"/>
                    <for variable="i" start="0" end="n-1" direction="inc" step="1">
                        <output expression="a[i]" newline="True"/>
                    </for>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
