                    <if expression="k==x">
                        <then>
                            <assign variable="a" expression="a&amp;char(b,l)"/>
                            <assign variable="s" expression="s+1"/>
                        </then>
                        <else/>
                    </if>
                </for>
                <for variable="y" start="97" end="123" direction="inc" step="1">
                    <if expression="k==y">
                        <then>
                            <assign variable="a" expression="a&amp;char(b,l)"/>
                            <assign variable="s" expression="s+1"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <output expression="a" newline="True"/>
        </body>
