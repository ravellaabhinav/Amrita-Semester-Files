    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, p, b" type="String" array="False" size=""/>
            <declare name="l, k, x, y, t, s, j" type="Integer" array="False" size=""/>
            <output expression="&quot;write&quot;" newline="True"/>
            <input variable="b"/>
            <assign variable="a" expression="&quot;&quot;"/>
            <assign variable="s" expression="0"/>
            <assign variable="t" expression="len(b)"/>
            <for variable="l" start="0" end="t-1" direction="inc" step="1">
                <assign variable="p" expression="char(b,l)"/>
                <assign variable="k" expression="tocode(p)"/>
