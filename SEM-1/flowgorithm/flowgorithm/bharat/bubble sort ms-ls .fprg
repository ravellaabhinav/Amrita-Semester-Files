    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, j, n, c, ms, ls, t, m" type="Integer" array="False" size=""/>
            <output expression="&quot;no.of num&quot;" newline="True"/>
            <input variable="n"/>
            <output expression="&quot;values&quot;" newline="True"/>
            <declare name="a" type="Integer" array="True" size="n"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <input variable="a[i]"/>
            </for>
            <for variable="i" start="1" end="n" direction="inc" step="1">
                <for variable="j" start="0" end="n-2" direction="inc" step="1">
                    <if expression="a[j]&gt;a[j+1]">
                        <then>
                            <assign variable="c" expression="a[j]"/>
                            <assign variable="a[j]" expression="a[j+1]"/>
                            <assign variable="a[j+1]" expression="c"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <output expression="&quot;tell&quot;" newline="True"/>
            <input variable="m"/>
            <assign variable="ms" expression="0"/>
            <assign variable="ls" expression="0"/>
            <for variable="i" start="n-1" end="n-m" direction="dec" step="1">
                <assign variable="ms" expression="ms+a[i]"/>
            </for>
            <for variable="i" start="0" end="m-1" direction="inc" step="1">
                <assign variable="ls" expression="ls+a[i]"/>
            </for>
            <assign variable="t" expression="ms-ls"/>
            <output expression="&quot;ans   &quot;&amp;t" newline="True"/>
        </body>
    </function>
</flowgorithm>
