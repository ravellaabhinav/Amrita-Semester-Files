    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="p, b, a, x, y, t, d, c" type="Real" array="False" size=""/>
            <output expression="&quot;enter a,b,c&quot;" newline="True"/>
            <input variable="a"/>
            <input variable="b"/>
            <input variable="c"/>
            <assign variable="d" expression="b^2-4*a*c"/>
            <if expression="d&gt;=0">
                <then>
                    <assign variable="p" expression="-b/(2*a)"/>
                    <assign variable="x" expression="p+(sqrt(d)/2*a)"/>
                    <assign variable="y" expression="p-(sqrt(d)/2*a)"/>
                    <output expression="x" newline="True"/>
                    <output expression="y" newline="True"/>
                </then>
                <else>
                    <assign variable="p" expression="-b/(2*a)"/>
                    <assign variable="t" expression="(sqrt(-d))*1/(2*a)"/>
                    <output expression="p&amp;&quot;+i&quot;&amp;t" newline="True"/>
                    <output expression="p&amp;&quot;-i&quot;&amp;t" newline="True"/>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
