    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i, t, p, x, k, n, s" type="Integer" array="False" size=""/>
            <output expression="&quot;no of values&quot;" newline="True"/>
            <input variable="n"/>
            <declare name="a" type="Integer" array="True" size="n"/>
            <output expression="&quot;enter values&quot;" newline="True"/>
            <assign variable="i" expression="0"/>
            <while expression="i&lt;n">
                <input variable="a[i]"/>
                <assign variable="i" expression="i+1"/>
            </while>
            <output expression="&quot;enter diff&quot;" newline="True"/>
            <input variable="k"/>
            <assign variable="s" expression="0"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <assign variable="t" expression="a[i]"/>
                <for variable="x" start="i+1" end="n-1" direction="inc" step="1">
                    <assign variable="p" expression="a[x]"/>
                    <if expression="(t-p)==k||(p-t)==k">
                        <then>
                            <assign variable="s" expression="s+1"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <output expression="&quot;no of values are   &quot;&amp;s" newline="True"/>
        </body>
    </function>
</flowgorithm>
