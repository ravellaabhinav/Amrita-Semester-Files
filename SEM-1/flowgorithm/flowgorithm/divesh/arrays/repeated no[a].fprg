    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n, i, p, s, t" type="Integer" array="False" size=""/>
            <input variable="n"/>
            <declare name="a" type="Integer" array="True" size="n"/>
            <for variable="i" start="0" end="n-1" direction="inc" step="1">
                <input variable="a[i]"/>
            </for>
            <assign variable="s" expression="0"/>
            <assign variable="p" expression="0"/>
            <assign variable="i" expression="0"/>
            <assign variable="t" expression="0"/>
            <while expression="i&lt;(n-1)&amp;&amp;t==0">
                <if expression="a[i]==a[i+1]">
                    <then>
                        <assign variable="t" expression="0"/>
                        <while expression="a[i]==a[i+1]&amp;&amp;t==0">
                            <if expression="i&lt;(n-2)">
                                <then>
                                    <assign variable="i" expression="i+1"/>
                                </then>
                                <else>
                                    <assign variable="i" expression="i-1"/>
                                    <assign variable="t" expression="1"/>
                                </else>
                            </if>
                        </while>
                        <output expression="a[i]" newline="True"/>
                    </then>
                    <else>
                        <assign variable="i" expression="i+1"/>
                    </else>
                </if>
            </while>
        </body>
    </function>
