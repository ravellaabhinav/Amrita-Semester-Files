            </for>
            <for variable="index" start="0" end="4" direction="inc" step="1">
                <output expression="&quot;enter the values of Array2[&quot; &amp; index &amp;&quot;]&quot;" newline="True"/>
                <input variable="Array2[index]"/>
            </for>
            <for variable="index" start="0" end="4" direction="inc" step="1">
                <assign variable="Array3[index]" expression="Array1[index]+Array2[index]"/>
