    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Array1, Array2, Array3" type="Integer" array="True" size="5"/>
            <declare name="index" type="Integer" array="False" size=""/>
            <for variable="index" start="0" end="4" direction="inc" step="1">
                <output expression="&quot;enter the values of Array1[&quot; &amp; index &amp;&quot;]&quot;" newline="True"/>
